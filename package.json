{
  "name": "hue-and-cry-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o build",
    "start": "next start",
    "prod": "next export",
    "test": "jest",
    "lint": "eslint './src/**/*.{ts,tsx}' && echo 'lint success'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}' && echo 'lint success'",
    "pre-commit-types": "tsc -p tsconfig.json",
    "pre-commit-lint": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit-types && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run pre-commit-lint --",
      "git add"
    ],
    "src/**/*.{json}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.3.1",
    "@mui/styles": "^5.3.0",
    "disqus-react": "^1.0.11",
    "next": "^12.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lazyload": "^3.2.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/classnames": "^2.2.11",
    "@types/gtag.js": "^0.0.4",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-lazyload": "^3.1.0",
    "classnames": "^2.2.6",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  }
}
