{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pre-commit-types": "tsc -p tsconfig.json",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "lint-staged": {
		"*.js": [
			"eslint --fix"
		],
		"*.{html,js}": [
			"prettier --write"
		]
	},
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit-types && lint-staged"
    }
  },
  "dependencies": {
    "@next/font": "13.0.7",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.7",
    "next": "13.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^17.0.11",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.1.0",
    "prettier": "^2.2.1",
    "typescript": "^4.9.4"
  }
}
